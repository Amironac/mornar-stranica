<style>
    html{
        scroll-behavior: smooth;
    }
    body {
    
    padding-bottom: 3rem;
    color: #5a5a5a;
    
    }
    .active{
      color:rgb(0, 102, 255);
    }
    .nav-item{
      margin-left:20px;

    }
    a{
      color:white;
      text-decoration:none;
      
    }
    a:hover{
      color:rgb(0, 102, 255);
      text-decoration:none;
    }
    body::-webkit-scrollbar-track
    {
        -webkit-box-shadow: inset 0 0 6px rgb(0,0,0);
        border-radius: 10px;
        height:200px;
        background-color: rgb(37, 37, 37);
    }
    
    body::-webkit-scrollbar
    {
        cursor: pointer;
        width: 9px;
        background-color: rgb(0, 0, 0);
    }
    
    body::-webkit-scrollbar-thumb
    {
        border-radius: 5px;
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.1);
        background-color: rgb(236, 236, 236);
    }
    .navbar{
        text-transform: uppercase;
        height: 210px;
        transition:.8s;
    }
    .navbar .img{
        width:120px;
        height:120px;
        transition: .5s;
        border-radius:50%;
        z-index: 20;
    }
    .dropdown-menu{
background: rgba(255, 255, 255, 0.788);
    }
    .dropdown-menu .dropdown-item{
      
    color:black;
  }
    .black{
        background-color: rgba(0, 0, 0, 0.3);
        height: 110px;
        padding: 5px 40px;
    }
    .black .img{
        width: 100px;
        height: 100px;
        transition: .5s;
        z-index:50;
    }
    #arrow{
      position:fixed;top:89%;left:90%;cursor:pointer;
      display: none;

      
      
    }
    
    
    @media(max-width:550px){
          .navbar-collapse{
        background: rgba(0, 0, 0, 0.726);
        padding:10px;
        margin-top:20px;
        border-radius: 5px;
        line-height: 3rem;
        
    }
    .navbar-collapse a{
        
        color:white;

    }
    }
</style>


